check process switchboard
  with pidfile /var/vcap/sys/run/bpm/proxy/switchboard.pid
  start program "/var/vcap/jobs/bpm/bin/bpm start proxy -p switchboard"
  stop program "/var/vcap/jobs/bpm/bin/bpm stop proxy -p switchboard" with timeout <%= p('cf_mysql.proxy.shutdown_delay').to_i + 30 %> seconds
  group vcap
